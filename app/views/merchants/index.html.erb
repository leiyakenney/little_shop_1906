<h1 align = "center">Merchants</h1>
<section class = "grid-container">
  <% @merchants.each do |merchant|%>
    <section class = "grid-item">
      <h2><%=link_to merchant.name, "/merchants/#{merchant.id}"%></h2>
    </section>
  <% end %>
  <section class = "grid-item">
    <h2><%= link_to "New Merchant", "/merchants/new" %></h2>
  </section>
</section>
